pipeline {
    agent any
    options {
        // Timeout counter starts AFTER agent is allocated
        timeout(time: 1, unit: 'SECONDS')
    }
    stages {
        stage('Example') {
            steps {
                docker rmi -f $(docker images |grep -v nginx |awk 'NR>1 {print $3}'|uniq)
            }
        }
    }
}